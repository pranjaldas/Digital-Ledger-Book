<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.android.example.baki_bohi">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CALL_PHONE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".tabs.HomeScreen"
            android:label="বাকী বহী"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".customer.ViewCustomer"
            android:label="@string/category_view_customer" />
        <activity
            android:name=".customer.AddCustomer"
            android:label="@string/category_add_customer" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShopKeeperProfile"
            android:label="@string/category_shopkeeper_profile" />
        <activity
            android:name=".MainHome"
            android:label="@string/app_name" />
        <activity
            android:name=".transaction.ViewTransaction"
            android:label="@string/category_transactions" />
        <activity
            android:name=".transaction.AddTransaction"
            android:label="@string/category_transactions" />
        <activity android:name=".registration.CreateAccount" />
        <activity android:name=".registration.OtpVarify" />
        <activity android:name=".registration.NewRegistration" />
        <activity android:name=".MainActivity" />
        <activity android:name=".registration.LogIn" />
    </application>

</manifest>